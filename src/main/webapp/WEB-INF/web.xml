<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AdminContactServlet</servlet-name>
  	<display-name>AdminContactServlet</display-name>
  	<description></description>
  	<servlet-class>com.contact.AdminContactServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteContactServlet</servlet-name>
  	<display-name>DeleteContactServlet</display-name>
  	<description></description>
  	<servlet-class>com.contact.DeleteContactServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AdminContactServlet</servlet-name>
  	<url-pattern>/AdminContactServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteContactServlet</servlet-name>
  	<url-pattern>/DeleteContactServlet</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>
